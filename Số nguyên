            Console.Write("Nhap N: ");
            int N = int.Parse(Console.ReadLine());
            int M = 0;
            while (N > 0)
            {
                int dv = N % 10;
                M = M * 10 + dv;
                N = N / 10;
            }
            Console.WriteLine($"So dao cua N la {M}");
            Console.ReadLine();
